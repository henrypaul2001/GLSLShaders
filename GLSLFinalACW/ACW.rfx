uniform vec3 velocity;
uniform float freq;
uniform float lifecycle;
uniform vec3 Translation;
uniform float particleSpread;
uniform float particleSystemHeight;
uniform float rotateFreq;
uniform float Mag;
uniform float gunAngle;

varying float fadingRate;

varying vec2 Texcoord;

mat3 RotateMatrixX(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(1.0, 0.0, 0.0,
               0.0, c, s,
               0.0, -s, c);
}

mat3 RotateMatrixY(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(c, 0.0, s,
               0.0, 1.0, 0.0,
               -s, 0.0, c);
}

mat3 RotateMatrixZ(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(c, s, 0.0, 
               -s, c, 0.0,
               0.0, 0.0, 1.0);
}

void main(void)
{
   vec4 bbPos = gl_Vertex;
   
   bbPos.xy *= billboardSize;
   
   // Billboarding
   vec3 Vx = viewInv[0].xyz;
   vec3 Vy = viewInv[1].xyz;
   
   bbPos.xyz = (bbPos.x * Vx + bbPos.y * Vy).xyz;
   
   float T = mod(freq * time, lifecycle);
   
   vec3 Pos = gl_Vertex;
   
   Texcoord = sign(bbPos.xy) * 0.5 + 0.5;
   
   float Angle = Mag * sin(freq * time);
   mat3 RotateX = RotateMatrixX(0);
   mat3 RotateY = RotateMatrixY(0);
   mat3 RotateZ = RotateMatrixZ(gunAngle);
   mat3 Rotation = RotateX * RotateY * RotateZ;
   
   velocity *= Rotation;
      
   Pos.xyz += velocity;
   
   bbPos.z += Translation.z;
   bbPos.y += Translation.y;
   bbPos.x += Translation.x + (3 * time);
   
   bbPos.xyz *= Rotation;
   
   bbPos.xyz += Pos * T;
   
   fadingRate = (1 - T / lifecycle);
   
   gl_Position = gl_ModelViewProjectionMatrix * bbPos;
}]]></RmShaderCode></RmGLShader>
