     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec2 Texcoord;

uniform vec3 Translation;
uniform vec3 Scale;
uniform float wingScale;
uniform float time;
uniform float freq;
uniform float bounce;

uniform float Mag;

mat3 RotateMatrixX(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(1.0, 0.0, 0.0,
               0.0, c, s,
               0.0, -s, c);
}

mat3 RotateMatrixY(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(c, 0.0, s,
               0.0, 1.0, 0.0,
               -s, 0.0, c);
}

mat3 RotateMatrixZ(float angle)
{
   float s = sin(angle);
   float c = cos(angle);
   
   return mat3(c, s, 0.0, 
               -s, c, 0.0,
               0.0, 0.0, 1.0);
}

void main( void )
{
   vec4 Pos = gl_Vertex;
   
   // Wing animation
   float Angle = Mag * sign(Pos.z) * sin(freq * time);
   
   mat3 RotateX = RotateMatrixX(Angle);
   mat3 RotateY = RotateMatrixY(0);
   mat3 RotateZ = RotateMatrixZ(0);
   mat3 Rotation = RotateX * RotateY * RotateZ;
   
   if (Pos.x < 25 && Pos.x > -25)
   {
      if (Pos.y < 5 && Pos.y > -5)
      {
         Pos.z *= wingScale;
         Pos.xyz *= Rotation;
      }
   }
   
   Pos.xyz -= vec3(0.5, 0.5, 0.5);
   Pos.xyz *= Scale;
   Pos.xyz += vec3(0.5, 0.5, 0.5);
   
   Pos.xyz += Translation;
   
   // Movement
   Pos.y += 2 * sin(bounce * time);
   Pos.x -= 7 * time;

   gl_Position = gl_ModelViewProjectionMatrix * Pos;
   Texcoord    = gl_MultiTexCoord0.xy;
}]]></RmShaderCode></RmGLShader>
