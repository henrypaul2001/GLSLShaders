     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D opacity; //fire shape
uniform sampler2D distortion; //fire distortion
uniform sampler2D base; //fire base

uniform vec2 V;
uniform vec2 scaling;
uniform vec2 frequency;
uniform vec2 windV;
uniform float noiseIntensity;

varying vec2 texCoord;
uniform float time;

void main(void)
{
   vec2 xy = texCoord;
   xy.y = 1 - xy.y;
   xy.x -= 0.5;
   xy *= scaling;
   xy.x += 0.5;
   xy.x += 0.1 * xy.y * sin(frequency.x * xy.y + time);
   xy.y += 0.1 * xy.y * sin(frequency.y * xy.x + time);
   
   vec2 xy1 = texCoord;
   xy1.y = 1 - xy1.y;
   xy1 += windV * time;
   vec4 fireShape = texture2D(opacity, xy);
   vec4 fireNoise = texture2D(distortion, xy1);
   vec4 fireColour = texture2D(base, vec2(texCoord.x, 1 - texCoord.y));
   
   gl_FragColor = noiseIntensity * (1 - xy.y) * fireShape * fireNoise * fireColour;
}]]></RmShaderCode></RmGLShader>
